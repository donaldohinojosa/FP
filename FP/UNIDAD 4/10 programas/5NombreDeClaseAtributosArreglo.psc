Proceso NombreDeClaseAtributosArreglo
	
	Definir clase,atributos Como Caracter;
	Escribir "Ingresa la clase"
	Leer clase;
	Escribir "Ingresa sus atributos"
	Leer atributos;
	
	Dimension nombres(5);
	
	nombres(1)<-"atributos";
	nombres(2)<-"atributos";
	nombres(3)<-"atributos";
	nombres(4)<-"atributos";
	nombres(5)<-"atributos";
	
	Para i<-1 Hasta 5 Con Paso 1 Hacer
	
		
		Escribir "El nombre que hay en la posicion ",i," es: ",nombres(1);
	FinPara
	
	
	
FinProceso
	