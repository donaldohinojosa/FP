Proceso AprobadoReprobado
	Definir a,b,c,d,e Como Entero
	definir f Como Real;
	Escribir "Ingrese primera calificacion : ";
	leer a;
	Escribir "Ingrese segunda calificacion : ";
	leer b;
	Escribir "Ingrese tercera calificacion : ";
	leer c;
	Escribir "Ingrese cuarta calificacion : ";
	leer d;
	Escribir "Ingrese quinta calificacion : ";
	leer e;
	f<- (a+b+c+d+e)/5;
	Escribir "EL PROMEDIO ES : ",f;
	Si f>7 Entonces
		Escribir "USTED APROBO";
	SiNo
		Escribir "USTED REPROBO";
	FinSi
	
FinProceso
